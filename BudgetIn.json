                          "pattern": "[\\n\\s\\w\\W^$]"
                        }
                      }
                    },
                    "meta": {
                      "type": "object",
                      "additionalProperties": false,
                      "properties": {
                        "location": {
                          "type": "string",
                          "maxLength": 100,
                          "pattern": "[\\n\\s\\w\\W^$]"
                        }
                      }
                    }
                  }
                },
                "examples": {
                  "success": {
                    "description": "Success Add device",
                    "value": {
                      "message": "User Created",
                      "data": {
                        "token": "jncksdc7bdjb6r7ausvb",
                        "name": "example name",
                        "username": "example"
                      },
                      "meta": {
                        "location": "https://api/v1/user/1"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
